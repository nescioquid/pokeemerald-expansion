script _LilycoveCity_DepartmentStore_5F_EventScript_ClerkFarLeft {
  lock
  faceplayer
  call(_Common_EventScript_Clerk_Hello)
  waitmessage
  pokemartdecoration2(LilycoveCity_DepartmentStore_5F_Pokemart_Dolls)
