// Originally Swagger
script _SlateportCity_PokemonFanClub_EventScript_MoveTutor {
  lock
  faceplayer
  if (!flag(FLAG_MOVE_TUTOR_MOVES_RANDOMIZED)) {
    call(_Common_EventScript_RandomMoveTutorMoveGenerator)
    setflag(FLAG_MOVE_TUTOR_MOVES_RANDOMIZED)
  }
  if (var(VAR_RANDOMIZED_MOVE_TUTOR_MOVE_1) == MOVE_WATER_PULSE) {
    call(_Common_EventScript_WaterPulseTutor)
  } elif (var(VAR_RANDOMIZED_MOVE_TUTOR_MOVE_1) == MOVE_BULLET_SEED) {
    call(_Common_EventScript_BulletSeedTutor)
  } elif (var(VAR_RANDOMIZED_MOVE_TUTOR_MOVE_1) == MOVE_GIGA_DRAIN) {
    call(_Common_EventScript_GigaDrainTutor)
  } elif (var(VAR_RANDOMIZED_MOVE_TUTOR_MOVE_1) == MOVE_IRON_TAIL) {
    call(_Common_EventScript_IronTailTutor)
  } elif (var(VAR_RANDOMIZED_MOVE_TUTOR_MOVE_1) == MOVE_SHOCK_WAVE) {
    call(_Common_EventScript_ShockWaveTutor)
  } elif (var(VAR_RANDOMIZED_MOVE_TUTOR_MOVE_1) == MOVE_SNATCH) {
    call(_Common_EventScript_SnatchTutor)
  } else {
    return
  }
  release
  end
}

