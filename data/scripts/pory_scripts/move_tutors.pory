// Originally TM03
script _Common_EventScript_WaterPulseTutor {
  if (!flag(FLAG_TEMP_1)) {
  msgbox(format("The sound of the sea… It calms me.\p But sometimes, the crashing waves pound like a drum in my head.\p I call it a Water Pulse. Would your Pokémon like to hear it?"), MSGBOX_YESNO)
  if (!var(VAR_RESULT)) {
    msgbox(format("No?\N Then the ocean keeps its song to me."))
    release
    end
  }
  msgbox(format("Show me, then.\N Which Pokémon will learn the sea's beat?"))
  setvar(VAR_0x8005, MOVE_WATER_PULSE)
  call(MoveTutor_EventScript_OpenPartyMenu)
  if (var(VAR_RESULT) == 0) {
    msgbox(format("No?\N Then the ocean keeps its song to me."))
    release
    end
  }
  setflag(FLAG_TEMP_1)
  }
  msgbox(format("When your Pokémon sends ripples,\N remember the ocean is always nearby."))
}

