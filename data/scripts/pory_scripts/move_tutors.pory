script _Common_EventScript_RandomMoveTutorMoveGenerator {
  random(6)
  switch (var(VAR_RESULT)) {
    case 0: setvar(VAR_RANDOMIZED_MOVE_TUTOR_MOVE_1, MOVE_WATER_PULSE)
    case 1: setvar(VAR_RANDOMIZED_MOVE_TUTOR_MOVE_1, MOVE_BULLET_SEED)
    case 2: setvar(VAR_RANDOMIZED_MOVE_TUTOR_MOVE_1, MOVE_GIGA_DRAIN)
    case 3: setvar(VAR_RANDOMIZED_MOVE_TUTOR_MOVE_1, MOVE_IRON_TAIL)
    case 4: setvar(VAR_RANDOMIZED_MOVE_TUTOR_MOVE_1, MOVE_SHOCK_WAVE)
    case 5: setvar(VAR_RANDOMIZED_MOVE_TUTOR_MOVE_1, MOVE_SNATCH)
  }
  random(6)
  switch (var(VAR_RESULT)) {
    case 0: setvar(VAR_RANDOMIZED_MOVE_TUTOR_MOVE_2, MOVE_WATER_PULSE)
    case 1: setvar(VAR_RANDOMIZED_MOVE_TUTOR_MOVE_2, MOVE_BULLET_SEED)
    case 2: setvar(VAR_RANDOMIZED_MOVE_TUTOR_MOVE_2, MOVE_GIGA_DRAIN)
    case 3: setvar(VAR_RANDOMIZED_MOVE_TUTOR_MOVE_2, MOVE_IRON_TAIL)
    case 4: setvar(VAR_RANDOMIZED_MOVE_TUTOR_MOVE_2, MOVE_SHOCK_WAVE)
    case 5: setvar(VAR_RANDOMIZED_MOVE_TUTOR_MOVE_2, MOVE_SNATCH)
  }
  random(6)
  switch (var(VAR_RESULT)) {
    case 0: setvar(VAR_RANDOMIZED_MOVE_TUTOR_MOVE_3, MOVE_WATER_PULSE)
    case 1: setvar(VAR_RANDOMIZED_MOVE_TUTOR_MOVE_3, MOVE_BULLET_SEED)
    case 2: setvar(VAR_RANDOMIZED_MOVE_TUTOR_MOVE_3, MOVE_GIGA_DRAIN)
    case 3: setvar(VAR_RANDOMIZED_MOVE_TUTOR_MOVE_3, MOVE_IRON_TAIL)
    case 4: setvar(VAR_RANDOMIZED_MOVE_TUTOR_MOVE_3, MOVE_SHOCK_WAVE)
    case 5: setvar(VAR_RANDOMIZED_MOVE_TUTOR_MOVE_3, MOVE_SNATCH)
  }
  random(6)
  switch (var(VAR_RESULT)) {
    case 0: setvar(VAR_RANDOMIZED_MOVE_TUTOR_MOVE_4, MOVE_WATER_PULSE)
    case 1: setvar(VAR_RANDOMIZED_MOVE_TUTOR_MOVE_4, MOVE_BULLET_SEED)
    case 2: setvar(VAR_RANDOMIZED_MOVE_TUTOR_MOVE_4, MOVE_GIGA_DRAIN)
    case 3: setvar(VAR_RANDOMIZED_MOVE_TUTOR_MOVE_4, MOVE_IRON_TAIL)
    case 4: setvar(VAR_RANDOMIZED_MOVE_TUTOR_MOVE_4, MOVE_SHOCK_WAVE)
    case 5: setvar(VAR_RANDOMIZED_MOVE_TUTOR_MOVE_4, MOVE_SNATCH)
  }
  if (var(VAR_RANDOMIZED_MOVE_TUTOR_MOVE_1) == VAR_RANDOMIZED_MOVE_TUTOR_MOVE_2) {
    call(_Common_EventScript_RandomMoveTutorMoveGenerator)
  }
  if (var(VAR_RANDOMIZED_MOVE_TUTOR_MOVE_1) == VAR_RANDOMIZED_MOVE_TUTOR_MOVE_3) {
    call(_Common_EventScript_RandomMoveTutorMoveGenerator)
  }
  if (var(VAR_RANDOMIZED_MOVE_TUTOR_MOVE_1) == VAR_RANDOMIZED_MOVE_TUTOR_MOVE_4) {
    call(_Common_EventScript_RandomMoveTutorMoveGenerator)
  }
  if (var(VAR_RANDOMIZED_MOVE_TUTOR_MOVE_2) == VAR_RANDOMIZED_MOVE_TUTOR_MOVE_3) {
    call(_Common_EventScript_RandomMoveTutorMoveGenerator)
  }
  if (var(VAR_RANDOMIZED_MOVE_TUTOR_MOVE_2) == VAR_RANDOMIZED_MOVE_TUTOR_MOVE_4) {
    call(_Common_EventScript_RandomMoveTutorMoveGenerator)
  }
  if (var(VAR_RANDOMIZED_MOVE_TUTOR_MOVE_3) == VAR_RANDOMIZED_MOVE_TUTOR_MOVE_4) {
    call(_Common_EventScript_RandomMoveTutorMoveGenerator)
  }
}
